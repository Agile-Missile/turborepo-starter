  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "@hyperse/eslint-config-hyperse": "^1.0.9",
    "@tailwindcss/typography": "^0.5.10",
    "animejs": "3.2.2",
    "autoprefixer": "^10.4.19",
    "http-status-codes": "^2.3.0",
    "next": "^14.1.4",
    "next-auth": "4.24.7",
    "next-sitemap": "^4.2.3",
    "nextra": "2.13.4",
    "nextra-theme-docs": "2.13.4",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@armit/eslint-config-bases": "^0.1.4",
    "@types/animejs": "^3",
    "@types/node": "20.11.30",
    "@types/react": "^18.2.70",
    "eslint": "^9.10.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "prettier": "3.2.5",
    "typescript": "5.4.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
