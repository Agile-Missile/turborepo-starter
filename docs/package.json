{
  "name": "@agile/docs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "build:mdx": "yarn node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/build-mdx",
    "dev": "next dev -p 3000 -H 0.0.0.0",
    "sitemap": "next-sitemap",
    "start": "next start -p 3000 -H 0.0.0.0",
    "test": "echo 'No tests yet'",
    "lint": "eslint . --cache --cache-location .cache/eslint/docs.eslintcache",
    "fix-all-files": "eslint . --fix",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@hyperse/eslint-config-hyperse": "^1.1.3",
    "@tailwindcss/typography": "^0.5.15",
    "animejs": "3.2.2",
    "autoprefixer": "^10.4.20",
    "http-status-codes": "^2.3.0",
    "next": "^14.2.14",
    "next-auth": "4.24.8",
    "next-sitemap": "^4.2.3",
    "nextra": "3.0.2",
    "nextra-theme-docs": "3.0.2",
    "postcss": "^8.4.47",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.13"
  },
  "devDependencies": {
    "@armit/eslint-config-bases": "^0.1.6",
    "@types/animejs": "^3",
    "@types/node": "22.7.4",
    "@types/react": "^18.3.10",
    "eslint": "^9.11.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "prettier": "3.3.3",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
